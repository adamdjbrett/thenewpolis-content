# Uploads: images (fallback)
/wp-content/uploads/*      /images/wp-content/uploads/:splat 301

# Removed non-upload asset paths from WP
/wp-content/plugins/* / 410
/wp-content/themes/*  / 410
/wp-includes/*        / 410
/themes/*             / 410

{% for item in collections.all %}
  {% if item.data.redirect_from %}
    {% set froms = item.data.redirect_from %}
    {% if froms is string %}{% set froms = [froms] %}{% endif %}
    {% for from in froms %}
{{ from }} {{ item.url }} 301
    {% endfor %}
  {% endif %}
{% endfor %}

